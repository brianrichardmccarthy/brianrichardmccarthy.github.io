<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title><%- title %></title>
    <link><%- url %></link>
    <description><%- description %></description>
    <atom:link href="<%- url %>feed.xml" rel="self" type="application/rss+xml" />
    <% for(slug in public.programming.cpp._data) { %>
    <% var post = public.programming.cpp._data[slug] %>
      <item>
        <title><%- post.title %></title>
        <pubDate><%- Date(post.date) %></pubDate>
        <link><%- url %>/programming/cpp/<%- post %></link>
        <guid isPermaLink="true"><%- url %>/programming/cpp/<%- post %></guid>
      </item>
    <% } %>
    <% for(slug in public.programming.java._data) { %>
    <% var post = public.programming.java._data[slug] %>
      <item>
        <title><%- post.title %></title>
        <pubDate><%- Date(post.date) %></pubDate>
        <link><%- url %>/programming/java/<%- post %></link>
        <guid isPermaLink="true"><%- url %>/programming/java/<%- post %></guid>
      </item>
    <% } %>
  </channel>
</rss>
