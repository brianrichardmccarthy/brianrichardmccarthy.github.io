<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title><%- title %></title>
    <link><%- url %></link>
    <description><%- description %></description>
    <atom:link href="<%- url %>/feed.xml" rel="self" type="application/rss+xml" />
	<% for (dataFile in [public.programming.cpp._data, public.programming.java._data]) { %>
    <% for(slug in dataFile) { %>
    <% var post = dataFile[slug] %>
      <item>
        <title><%- post.title %></title>
		<description><![CDATA[<%- partial("blog/" + slug) %>]]></description>
        <pubDate><%- Date(post.date) %></pubDate>
        <link><%- url %>/blog/<%- post %></link>
        <guid isPermaLink="true"><%- url %>/blog/<%- post %></guid>
      </item>
    <% } %>
    <% } %>
  </channel>
</rss>
